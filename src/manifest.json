{
    "manifest_version": 2,

    "name": "Assignment Tracker",
    "description": "An extension to help track the various different details needed for an assignment",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Assignment Tracker"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}
